TextLabel.TextSize = 24.000
TextLabel.TextWrapped = true

Legacy.Name = "Legacy"
Legacy.Parent = Main
Legacy.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Legacy.Position = UDim2.new(0, 11, 0, 59)
Legacy.Size = UDim2.new(0, 112, 0, 36)
Legacy.Font = Enum.Font.SourceSans
Legacy.Text = "Legacy"
Legacy.TextColor3 = Color3.fromRGB(255, 255, 255)
Legacy.TextSize = 14.000
Legacy.TextWrapped = true
Legacy.MouseButton1Down:connect(function()
	repeat
		wait();
	until game:IsLoaded();
	loadstring(game:HttpGet('https://raw.githubusercontent.com/Legacy-Foundation/Legacy-Hub/main/.lua'))();
end)

UICorner.Parent = Legacy

